<openerp>
<data>

<record model="ir.ui.view" id="view_partner_form_socials">
    <field name="name">Partner form with socials</field>
    <field name="model">res.partner</field>
    <field name="inherit_id" ref="base.view_partner_form"/>
    <field name="arch" type="xml">
        <xpath expr="/form/sheet/group">
            <group name="social_fields"
                   string="Social">
                <field name="social_facebook" placeholder="Facebook" />
                <field name="social_linkedin" placeholder="Linkedin" />
                <field name="social_twitter" placeholder="Twitter" />
                <field name="social_youtube" placeholder="Youtube" />
                <field name="social_google_plus" placeholder="Google Plus" />
            </group>
        </xpath>
    </field>
</record>


</data>
</openerp>
